{
  "name": "soya",
  "version": "0.0.17",
  "description": "Hopefully hassle-free web front-end framework",
  "repository": {
    "type": "git",
    "url": "https://github.com/traveloka/soya.git"
  },
  "keywords": [
    "webpack",
    "isomorphic",
    "front-end",
    "framework",
    "soya"
  ],
  "main": "lib/index.js",
  "license": "MIT",
  "scripts": {
    "watch": "./node_modules/.bin/babel src/ --out-dir lib --source-maps --watch",
    "watch-starter": "./node_modules/.bin/babel src/ --out-dir \"../soya-starter/node_modules/soya/lib\" --source-maps --watch",
    "watch-starter-redux": "./node_modules/.bin/babel src/ --out-dir \"../soya-starter-redux/node_modules/soya/lib\" --source-maps --watch",
    "watch-tap": "./node_modules/.bin/babel src/ --out-dir \"../../tap/node_modules/soya/lib\" --source-maps --watch"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.24",
    "babel-eslint": "^4.1.1",
    "babel-plugin-flow-comments": "^1.0.9",
    "babel-runtime": "^5.8.20",
    "express": "^4.13.3"
  },
  "dependencies": {
    "memory-fs": "^0.2.0",
    "mustache": "^2.1.3",
    "rimraf": "^2.4.3"
  }
}
