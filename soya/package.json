{
  "name": "soya",
  "version": "0.0.75",
  "description": "Hopefully hassle-free web front-end framework",
  "repository": {
    "type": "git",
    "url": "https://github.com/traveloka/soya.git"
  },
  "keywords": [
    "webpack",
    "isomorphic",
    "universal",
    "front-end",
    "framework",
    "soya"
  ],
  "main": "lib/index.js",
  "license": "MIT",
  "scripts": {
    "build": "./node_modules/.bin/babel src/ --out-dir lib --source-maps --copy-files",
    "watch": "./node_modules/.bin/babel src/ --out-dir lib --source-maps --copy-files --watch",
    "watch-starter": "./node_modules/.bin/babel src/ --out-dir \"../soya-starter/node_modules/soya/lib\" --source-maps --copy-files --watch",
    "watch-starter-redux": "./node_modules/.bin/babel src/ --out-dir \"../soya-starter-redux/node_modules/soya/lib\" --source-maps --copy-files --watch",
    "watch-test-integration": "./node_modules/.bin/babel src/ --out-dir \"../test/integration/node_modules/soya/lib\" --source-maps --copy-files --watch"
  },
  "peerDependencies": {
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-2": "^6.18.0",
    "babel-plugin-transform-export-extensions": "^6.8.0",
    "babel-plugin-transform-object-assign": "^6.8.0",

    "css-loader": "^0.19.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "mime": "^1.3.4",
    "node-sass": "^3.10.0",
    "file-loader": "^0.8.4",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-addons-update": "^0.14.6",
    "sass-loader": "^4.0.2",
    "socket.io": "^1.4.8",
    "socket.io-client": "^1.4.8",
    "socket.io-redis": "^1.1.1",
    "source-map-support": "^0.3.2",
    "style-loader": "^0.12.4",
    "webpack": "^1.12.2"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-2": "^6.18.0",
    "babel-plugin-transform-export-extensions": "^6.8.0",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "express": "^4.13.3"
  },
  "dependencies": {
    "crypto-js": "^3.1.8",
    "html-entities": "^1.2.0",
    "js-yaml": "^3.4.3",
    "memory-fs": "^0.2.0",
    "rimraf": "^2.4.3"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-2"
    ],
    "plugins": [
      "transform-export-extensions",
      "transform-object-assign"
    ]
  }
}
